{
  "rules": {
    "stashes": {
      "$uid": {
        // User can read or write if the stash belongs to him or is in share list
        ".read": "$uid === auth.uid || data.child('share/' + auth.uid).val() === true",
        ".write": "$uid === auth.uid || data.child('share/' + auth.uid).val() === true"
      }
    }
  }
}